- Admin:
    - Moon Lamp:
        href: http://docker.home.lab:9015/
        icon: roundcube.png
        ping: 192.168.1.247
    - Vaultwarden:
        href: https://vaultwarden.home.lab/
        icon: vaultwarden.png
        siteMonitor: https://vaultwarden.home.lab/
    - Syncthing:
        href: https://syncthing.home.lab/
        icon: syncthing.png
        siteMonitor: https://syncthing.home.lab/
    - Gitea:
        href: https://gitea.home.lab/
        icon: gitea.png
        siteMonitor: https://gitea.home.lab/

- Media:
    - Jellyfin:
        href: https://jellyfin.home.lab/
        icon: jellyfin.png
        siteMonitor: https://jellyfin.home.lab/
    - Audiobookshelf:
        href: https://audiobookshelf.home.lab/
        icon: audiobookshelf.png
        siteMonitor: https://audiobookshelf.home.lab/
    - Filebrowser:
        href: https://filebrowser.home.lab/
        icon: filebrowser.png
        siteMonitor: https://filebrowser.home.lab/
