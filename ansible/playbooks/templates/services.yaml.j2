- Server:
    - Portainer:
        href: "https://pc.home.lab:9443/#!/auth"
        icon: portainer.png
        ping: 192.168.1.2
        widget:
          type: portainer
          url: https://pc.home.lab:9443
          env: 6
          key: {{ portainer_key }}
    - SRV:
        href: https://srv.home.lab:8006/
        icon: proxmox.png
        description: PROD
        ping: 192.168.1.12
        widget:
          type: proxmox
          url: https://srv.home.lab:8006
          username: homepage@pam!homepage
          password: {{ proxmox_key }}
    - PiHole:
        href: http://pihole.home.lab/admin
        icon: pi-hole.png
        ping: 192.168.1.40
        siteMonitor: http://pihole.home.lab/admin
        widget:
          type: pihole
          url: http://pihole.home.lab
          key: {{ pihole_key }}
    - PVE:
        href: https://pve.home.lab:8006/
        icon: proxmox.png
        description: DEV
        ping: 192.168.1.3
    - Router:
        href: https://192.168.1.1/
        icon: router.png
        description: AX1800 Wi-Fi 6 Router
        ping: 192.168.1.1
    - EmonESP:
        href: http://192.168.1.242/
        icon: lnbits.png
        ping: 192.168.1.242

