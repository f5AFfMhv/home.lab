---
# For configuration options and examples, please see:
# https://gethomepage.dev/latest/configs/settings

background:
  image: https://wallpaperaccess.com/full/2475175.jpg
  brightness: 100
  opacity: 90
theme: dark
cardBlur: md

useEqualHeights: true
headerStyle: boxed
hideVersion: true
showStats: true
target: _self

providers:
  openweathermap: {{ openweathermap_key }}
  finnhub: {{ finnhub_key }}

layout:
  - Calendar:
      style: row
      columns: 1
      initiallyCollapsed: true
  - Bookmarks:
      style: row
      columns: 20
      iconsOnly: true
      initiallyCollapsed: true
  - Services:
      style: row
      columns: 4
      header: false
  - Server:
      style: row
      columns: 2
  - Bottom:
      Network:
      Download:
      Arr Stack:
