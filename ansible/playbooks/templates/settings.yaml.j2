---
# For configuration options and examples, please see:
# https://gethomepage.dev/latest/configs/settings

background:
  image: https://images.unsplash.com/photo-1511207538754-e8555f2bc187?q=80&w=2412&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D
  brightness: 100
  opacity: 90
theme: dark
cardBlur: md

useEqualHeights: true
headerStyle: boxed
hideVersion: true
showStats: true

providers:
  openweathermap: {{ openweathermap_key }}

layout:
  Server:
    icon: mdi-server-#FFFFFF
  Media:
    icon: mdi-folder-multiple-image-#FFFFFF
  Arr Stack:
    icon: mdi-pirate-#FFFFFF
  Calendar:
    icon: mdi-calendar-month-#FFFFFF
  Download:
    icon: mdi-cloud-download-outline-#FFFFFF
  Monitoring:
    icon: mdi-list-status-#FFFFFF
